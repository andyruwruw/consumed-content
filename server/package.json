{
  "name": "consumed-content-server",
  "version": "1.0.0",
  "description": "Serverless API for consuming content.",
  "main": "index.js",
  "scripts": {
    "build": "tsc --build",
    "start": "vercel dev",
    "clean": "rimraf ./dist && echo 'All clean now!'",
    "test": "jest --colors",
    "test:verbose": "jest --verbose --colors",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "database:setup": "nodemon --config nodemon.json src/utils/setup.ts",
    "database:reset": "nodemon --config nodemon.json src/utils/reset-database.ts",
    "database:wipe": "nodemon --config nodemon.json src/utils/wipe-database.ts",
    "database:create-data": "nodemon --config nodemon.json src/utils/create-sample-data.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyruwruw/consumed-content.git"
  },
  "author": "Andrew Young & Carlos Muro",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andyruwruw/consumed-content/issues"
  },
  "homepage": "https://github.com/andyruwruw/consumed-content#readme",
  "dependencies": {
    "@vercel/node": "^1.12.1",
    "axios": "^0.26.0",
    "bcrypt": "^5.0.1",
    "dotenv": "^16.0.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.5.5"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.0",
    "@types/jest": "^27.4.1",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.13.0",
    "eslint": "^8.10.0",
    "eslint-config-airbnb-typescript": "^16.1.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.6.0",
    "typescript": "^4.6.2"
  }
}
